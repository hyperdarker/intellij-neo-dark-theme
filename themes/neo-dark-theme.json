	"tokenColors": [
		
		{
			"scope": [
				"punctuation.definition.parameters.begin.bracket.round",
				"punctuation.definition.parameters.end.bracket.round",
				"storage.modifier.package",
				"storage.modifier.import",
				"variable.parameter"],
			"settings": {
				"foreground": "#A9B7C6"
			}
		},{
			"scope": "meta.method.identifier",
			"settings": {
				"foreground": "#FFC66D"
			}
		},
		{
			"scope": ["variable.other.object"],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"scope": ["support.type.property-name",
			"variable.language.special.self.python",
					"constant"],
			"settings": {
				"foreground": "#9876AA"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#6897BB"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#808080"
			}
		},{

			"scope": ["string"],
			"settings": {
				"foreground": "#6A8759"
			}
		},
		{
			"scope": ["storage.modifier",
			"keyword.control",
			"storage.type",
			"entity.name.tag.html",
			"constant.language",
			"constant.character.escape",
			"keyword.other.import",
			"entity.name.tag",
			"keyword.other.package",
			"punctuation.terminator",
			"punctuation.separator.delimiter",
			"punctuation.separator.comma"],
			"settings": {
				"foreground": "#CC7832"
			}
		},{
			"scope": ["punctuation.definition.annotation",
				"storage.type.annotation"],
			"settings": {
				"foreground": "#BBB529"
			}
		},    {
			"name": "Comments",
			"scope": [
			  "comment markup.link",
			  "markup.quote.markdown",
			  "comment, punctuation.definition.comment",
			  "comment.block.javadoc.java",
			  "comment.line.double-slash.java",
			  "comment.line.double-slash.rust",
			  "comment.line.documentation.rust"
			],
			"settings": {
			  "foreground": "#5d5f63"
			}
		  },
		  {
			"name": "!important css",
			"scope": ["keyword.other.important.css", "keyword.other.important.scss"],
			"settings": {
			  "foreground": "#FA5762"
			}
		  }
	]
}
